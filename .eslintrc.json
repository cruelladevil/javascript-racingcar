{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base/legacy"
  ],
  "rules": {
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-return-await": "off"
  }
}
